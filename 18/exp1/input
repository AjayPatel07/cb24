#include<stdio.h>
void main()
{
    int number = 0;
    float num2 = ;
    char letter = '';

    printf("enter value of number ");
    scanf("%d",number);

    printf("enter value of num2 ");
    scanf("%f",num2);
    fflush(stdin);

    printf("enter any letter");
    scanf("%c",letter);

    printf("the value of number is %d ",number);
    printf("\n the value of num2 is %.2f ", num2);
    printf("\n the value of letter is %c",letter);

}
